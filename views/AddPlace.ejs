                    <form action="/AddPlace" method="post">
                        <p>Place Id: </p> <input type="text" name="BusinessId" placeholder="" required=""/><br>
                        <p>Business Name: </p> <input type="text" name="BusinessName" placeholder="" required=""/><br>
                        <p>City: </p> <input type="text" name="City" placeholder="" required=""/>
                        <p>Address: </p> <input type="text" name="Address" placeholder="" required=""/>
                        <p>Occupatione: </p>
                        <br><a>Weekday: </a><input type="text" name="OccupationWeekday" placeholder="" required=""/> <a>Weekend: </a><input type="text" name="OccupationWeekend" placeholder="" required=""/>
                        <p>Menu website: </p> <input type="text" name="Menu" placeholder="" required=""/>
                        <p>Kosher: </p> <input type="text" name="Kosher" placeholder="" required=""/>
                        <p>Business Hours: </p> <input type="text" name="BusinessHours" placeholder="" required=""/>


                        <br><p>BusinessType: </p>
                        <br> <a>Type: </a><input type="text" name="Type" placeholder="" required=""/>
                        <a>FoodType: </a><input type="text" name="FoodType" placeholder="" required=""/>
                        <br>  <br><a>ChefName: </a><input type="text" name="ChefName" placeholder="" required=""/>
                        <a>Delivery: </a><input type="text" name="Delivery" placeholder="" required=""/>
                        <br><input type="submit" value="Add Place">
                    </form>
                    <br><br> <p><font size="8" color="blue"><u><%=userNotfoundMsg.msg%></u></font></p>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <!-- end content -->
    </div>
</div>
