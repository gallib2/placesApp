<body>
<!-- header -->
<div class="header_bg">
    <div class="container">
        <div class="header">
            <div class="head-t">
                <div class="logo">
                    <font size="2" ><a><u>Hello <%= user.FullName%></u> </a><br>
                        <a >Admin's Name: <%= user.UserName%> </a></font>
                </div>
                <!-- start header_right -->

                <div class="clearfix"> </div>
            </div>
            <!-- start header menu -->
            <ul class="megamenu skyblue">
                <li class="active grid"><a class="color1" href="../../adminDashboard">Home</a></li>
