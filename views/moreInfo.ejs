Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Playfair+Display:400,700,900' rel='stylesheet' type='text/css'>
    <!-- start menu -->
    <link href="/stylesheets/dashboard/megamenu.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="/stylesheets/dashboard/etalage.css">
    <script type="text/javascript" src="/javascripts/dashboard/megamenu.js"></script>
    <script>$(document).ready(function(){$(".megamenu").megamenu();});</script>
    <script src="/javascripts/dashboard/jquery.etalage.min.js"></script>
    <script src="/javascripts/dashboard/menu_jquery.js"></script>
    <script>
        /*jQuery(document).ready(function($){

            $('#etalage').etalage({
                thumb_image_width: 300,
                thumb_image_height: 400,
                source_image_width: 900,
                source_image_height: 1200
                //show_hint: true
            });

        });*/
    </script>

</head>
<body>
<!-- header_top -->

<!-- header -->
<div class="header_bg">
    <div class="container">
        <div class="header">
            <div class="head-t">
                <div class="logo">
                    <font size="2" ><a><u>Hello <%= user.FullName%></u> </a><br>
                        <a >User Name: <%= user.UserName%> </a></font>
                </div>
                <!-- start header_right -->

            </div>
            <!-- start header menu -->
            <ul class="megamenu skyblue">
                <li class="active grid"><a class="color1" href="/dashboard">Home</a></li>
                <li class="grid"><a class="color2" href="#">My hang-out places</a>
                    <div class="megapanel">
                        <div class="row">
                            <div class="col1">
                                <div class="h_nav">
                                    <ul>
                                        <li><a href="/userPlaces"> Show my hang-out places</a></li>
                                        <li><a href="women.html">Search a hang-out place</a></li>
                                        <li><a href="women.html">Add new hang-out place</a></li>
                                        <li><a href="women.html">Remove hang-out place</a></li>
                                    </ul>
                                </div>
                            </div>

                        </div>

                    </div>
                </li>
                <li><a class="color4" href="#">My friends</a>
                    <div class="megapanel">
                        <div class="row">
                            <div class="col1">
                                <div class="h_nav">
                                    <ul>
                                        <li><a href="/userFriends">Show my friends list</a></li>
                                        <li><a href="women.html">Search a friend</a></li>
                                        <li><a href="women.html">Add a friend</a></li>
                                        <li><a href="women.html">Remove a friend</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>
<!-- content -->
<div class="container">
    <div class="women_main">
        <!-- start content -->
        <div class="row single">
            <div class="col-md-9 det">
                <div class="single_left">

                    <div class="desc1 span_3_of_2">
                        <h3><%= place.BusinessName %></h3>
                        <span class="brand"> City: <a> <%= place.City %></a></span>
                        <br>
                        <span class="code"> <%= place.Address %> </span><br />
                        <span class="price-new"> Sales: </span>
                        <p> <span class = "price-new"> <%= place.Sales %> </span> </p>
                        <div class="price">
                            <span class="brand"> Opening hours:  <a> <%= place.BusinessHours %></a> </span><br />
                            <span class="brand"> Occupation:
                                <a> <b>weekday:</b>  <%= place.Occupation.weekday %>
                                    <b>Weekend:</b> <%= place.Occupation.Weekend %> </a> </span><br />
                            <span class="brand"> Business Type:  <a> <%= place.BusinessType %></a> </span><br />
                            <span class="brand"> Food type:  <a> <%= place.FoodType %></a> </span><br />
                            <span class="brand"> Chef name: <a> <%= place.ChefName %></a> </span><br />
                            <span class="brand"> Delivery: <a> <% if(place.Delivery){  %>
                                     <a>yes</a>
                                <% } else {%>
                                    <a>no</a>
                                    <% }  %>
                                </a> </span><br />
                            <span class="brand"> Kosher: <a> <% if(place.Kosher){  %>
                                    <a>yes</a>
                                    <% } else {%>
                                    <a>no</a>
                                    <% }  %>
                                </a> </span>
                        </div>
                        <div class="det_nav1">
                            <h4><a href=<%= place.Menu %> >Website Menu</a></h4>
                            <div class="btn_form">
                                <a href="/userPlaces">Go to my places</a>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>

        </div>
        <!-- end content -->
    </div>
</div>


</body>
</html>