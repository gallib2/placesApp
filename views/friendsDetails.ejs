<!DOCTYPE HTML>
<html>
<head>
    <title>Friend's profile</title>
    <link href="/stylesheets/dashboard/bootstrap.css" rel='stylesheet' type='text/css' />
    <!-- jQuery (necessary JavaScript plugins) -->
    <script type='text/javascript' src="/javascripts/dashboard/jquery-1.11.1.min.js"></script>
    <!-- Custom Theme files -->
    <link href="/stylesheets/dashboard/style.css" rel='stylesheet' type='text/css' />
    <!-- Custom Theme files -->
    <!--//theme-style-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Gretong Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Playfair+Display:400,700,900' rel='stylesheet' type='text/css'>
    <!-- start menu -->
    <link href="/stylesheets/dashboard/megamenu.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="/stylesheets/dashboard/etalage.css">
    <script type="text/javascript" src="/javascripts/dashboard/megamenu.js"></script>
    <script>$(document).ready(function(){$(".megamenu").megamenu();});</script>
    <script src="/javascripts/dashboard/jquery.etalage.min.js"></script>
    <script src="/javascripts/dashboard/menu_jquery.js"></script>
    <script>
        jQuery(document).ready(function($){

            $('#etalage').etalage({
                thumb_image_width: 300,
                thumb_image_height: 400,
                source_image_width: 900,
                source_image_height: 1200,
                show_hint: true,
                click_callback: function(image_anchor, instance_id){
                    alert('Callback example:\nYou clicked on an image with the anchor: "'+image_anchor+'"\n(in Etalage instance: "'+instance_id+'")');
                }
            });

        });
    </script>

</head>
<body>
<!-- header -->
<div class="header_bg">
<div class="container">
    <div class="header">
        <div class="head-t">
            <div class="logo">
                <font size="2" ><a><u>Hello <%= user.FullName%></u> </a><br>
                    <a >User Name: <%= user.UserName%> </a></font>
            </div>
            <!-- start header_right -->

            <div class="clearfix"> </div>
        </div>
        <!-- start header menu -->
        <ul class="megamenu skyblue">
            <li class="active grid"><a class="color1" href="../../dashboard">Home</a></li>
            <li class="grid"><a class="color2" href="#">My hang-out places</a>
                <div class="megapanel">
                    <div class="row">
                        <div class="col1">
                            <div class="h_nav">
                                <ul>
                                    <li><a href="/userPlaces"> Show my hang-out places</a></li>
                                    <li><a href="/AddMyPlace">Add new hang-out place</a></li>
                                </ul>
                            </div>
                        </div>

                    </div>

                </div>
            </li>
            <li><a class="color4" href="#">My friends</a>
                <div class="megapanel">
                    <div class="row">
                        <div class="col1">
                            <div class="h_nav">
                                <ul>
                                    <li><a href="/userFriends">Show my friends list</a></li>
                                    <li><a href="/AddMyFriend">Add a friend</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
            </li>
        </ul>
    </div>
</div>
</div>
<!-- content -->
<div class="container">
    <div class="women_main">
        <!-- start content -->
        <div class="row single">
            <div class="col-md-9 det">
                <div class="single_left">

                    <div class="desc1 span_3_of_2">
                        <h3>"<%= friend.UserName%>" profile:</h3>
                        <span class="brand">User Name: <a><%= friend.UserName%> </a></span><br>
                        <span class="brand">Full Name: <a><%= friend.FullName%> </a></span><br>
                        <%if(places.length == 0){%>
                        <a>NO PLACES TO PRIVIEW</a>
                        <%}else{%>
                        <span class="brand">Favorite Places: <a><% places.forEach(function(place, index, arr){ %>
                                   <br><a ><%=place.BusinessName%></a>
                                   <a href=<%=place.Menu%>><u><font size="2" color="blue">Go To Menu</font></u></a>
                                <%})%> </a></span>
                        <%}%>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <!-- end content -->
    </div>
</div>
</body>
</html>